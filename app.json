{
  "expo": {
    "orientation": "default",
    "scheme": "fenixretail",
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "name": "Fenix Retail",
    "slug": "fenix-retail-2",
    "version": "2.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "web": {
        "favicon": "./assets/favicon.png",
        "bundler": "metro"
      }
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.mquerkacademy.fenixretail2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mquerkacademy.fenixretail2"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "729a3a1b-a37b-4750-822d-db47d100df1d"
      }
    }
  }
}
