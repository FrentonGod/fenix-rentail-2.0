{
  "expo": {
    "orientation": "landscape",
    "scheme": "fenixrentail",
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "name": "fenix-rentail-2.0",
    "slug": "fenix-rentail-2.0",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "web": {
        "favicon": "./assets/favicon.png",
        "bundler": "metro"
      }
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    }
  }
}
